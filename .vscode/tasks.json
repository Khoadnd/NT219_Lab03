{
  "tasks": [
    {
      "type": "shell",
      "label": "WHAT THE FUCK",
      "command": "/usr/bin/g++-10",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "${file}",
        "${fileDirname}/src/*.cpp",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}",
        "-std=c++20",
        "-pthread"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "?"
    }
  ],
  "version": "2.0.0"
}